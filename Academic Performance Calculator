#include <stdio.h>
int main() {
    int eng, math, science, computer, history, marks_obtained;
    float average;
    scanf("%d%d%d%d%d",&eng,&math,&science,&computer,&history);
    marks_obtained=eng+ math+ science+ computer+ history;
    average=marks_obtained/5;
    printf("%d %.2f %.2f",marks_obtained, average, average);
    return 0;
}
